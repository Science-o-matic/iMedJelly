<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical">
    <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_height="wrap_content" >

        <ImageView
                android:layout_width="385dp"
                android:layout_height="120dp"
                android:id="@+id/ij9aa"
                android:contentDescription="@string/nj9a"
                android:scaleType="fitXY"
                android:src="@drawable/explicacion9a"/>

        <TextView
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:typeface="sans"
                android:id="@+id/tj9aa"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:background="#FCD9B9"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/j9a1" />

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="@string/j9a2"
                android:id="@+id/tj9ab"
                android:layout_gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp" />

        <TableLayout
                android:id="@+id/tableLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:stretchColumns="*">

            <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/j9a3"
                        android:id="@+id/tj9ac"
                        android:layout_gravity="left|center_vertical"
                        android:paddingLeft="10dp"
                android:paddingRight="10dp"
                        android:textStyle="bold"/>

                <ImageView
                        android:layout_width="300dp"
                        android:layout_height="40dp"
                        android:id="@+id/ij9ab"
                        android:contentDescription="@string/ij9a"
                        android:layout_gravity="left"
                        android:src="@drawable/ninguna"/>
            </TableRow>

        </TableLayout>

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/j9a4"
                android:id="@+id/tj9ad"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_gravity="center"/>

        <TextView
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:text="@string/j9a5"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/tj9ae"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textStyle="bold"/>

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/tj9af"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_gravity="center"
                android:text="@string/j9a6"/>

        <TextView
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:text="@string/j9a7"
                android:id="@+id/tj9ag"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textStyle="bold"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"/>

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/j9a8"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/tj9ah"/>

        <TextView
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:text="@string/j9a9"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:id="@+id/tj9ai"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textStyle="bold"/>

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="@string/j9a10"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/tj9aj"
                android:singleLine="false"/>

    </LinearLayout>
</ScrollView>